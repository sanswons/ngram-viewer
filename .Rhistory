else if (tag %in% ad_tags){
syns = c(syns,qdap::synonyms(w, return.list = FALSE))
syns = c(syns,wordnet::synonyms(w,'ADJ'))
}
suggestions  = create_synonyms_phrase(query, unlist(syns), w)
#print(unlist(suggestions))
}
return(suggestions)
